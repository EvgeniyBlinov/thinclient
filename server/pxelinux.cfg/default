default thinclient
prompt 0

# DEBUG with serial console
# console=tty1 console=ttyS0,115200n8
label thinclient
    kernel vmlinuz
    append boot=ram initrd=initrd.img rootproto=http rooturl=/rootfs.squashfs homeproto=http homeurl=/home.tar.gz rdpservers=dc1:dc1.example.net:3389;dc2:dc2.example.net:5555:/sec:rdp
